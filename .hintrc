{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 26",
    "not safari <= 26"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "css-prefix-order": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[referrerpolicy=no-referrer-when-downgrade]"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ]
  }
}